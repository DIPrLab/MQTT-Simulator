{
	"BROKER_URL": "test.mosquitto.org",
	"BROKER_PORT": 1883,
	"INCLUDE_MESSAGE_ID": true,
	"INCLUDE_TIMESTAMP": true,
	"METADATA_FIELD_PREFIX": "_",
	"TOPIC_DIMENSIONS": [
		{ "name": "lamp", "range": [1, 5] },
		{ "name": "room", "list": ["kitchen", "living", "bedroom"] },
		{ "name": "air_quality", "list": ["pm25", "co2"] },
		{ "name": "temperature", "list": ["roof", "basement"] },
		{ "name": "freezer", "list": ["main", "backup"] },
		{ "name": "location", "list": ["office", "warehouse"] }
	],
	"DATA_SCHEMAS": {
		"lamp": {
			"on": {"type": "bool"},
			"temperature": {"type": "int", "min": 2700, "max": 6500}
		},
		"air_quality": {
			"pm25": {"type": "float", "min": 0, "max": 1},
			"co2": {"type": "float", "min": 400, "max": 2000}
		},
		"temperature": {
			"value": {"type": "float", "min": 20, "max": 55}
		},
		"freezer": {
			"temp": {"type": "float", "min": -30, "max": 0}
		},
		"location": {
			"lat": {"type": "float", "min": -180, "max": 180},
			"long": {"type": "float", "min": -90, "max": 90}
		}
	},
	"TIME_INTERVAL": 4,
	"TOPIC_TYPES": ["lamp", "air_quality", "temperature", "freezer", "location"],
	"TOPIC_TYPE_DIMENSIONS": {
		"lamp": ["lamp", "room"],
		"air_quality": ["air_quality", "room"],
		"temperature": ["temperature", "room"],
		"freezer": ["freezer", "location"],
		"location": ["location"]
	},
	"SUBSCRIBERS": {
		"USERS": ["user1", "user2", "user3"],
		"PASSWORDS": ["password1", "password2", "password3"],
		"TOPICS": [
			"kitchen/#",
			"living/#",
			"bedroom/#",
			"office/#",
			"warehouse/#"
		]
	}
}